	<!-- startup action - load the first scene -->
	<autorotate enabled="true" waittime="2.0" speed="-10.0" horizon="0.0" />
	
	<events name="onautorotateevents" keep="true" onautorotateoneround="startup()"/>
	
	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
	</action>

	<action name="hide_vtourskin_all" autorun="onstart">
	        delayedcall(2.0,skin_hideskin());
	</action>


	
